    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-coveralls": "1.0.1",
    "grunt-eslint": "18.1.0",
    "grunt-flowbin": "0.0.5",
    "grunt-mocha-test": "0.12.7",
    "grunt-newer": "1.2.0",
    "grunt-webpack": "1.0.11",
    "load-grunt-configs": "1.0.0",
    "load-grunt-tasks": "3.5.0",
    "mocha": "2.5.3",
