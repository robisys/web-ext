    "stream-to-promise": "1.1.0",
    "tmp": "0.0.28",
    "watchpack": "1.1.0",
    "yargs": "4.7.1",
    "zip-dir": "1.0.2"
  },
  "devDependencies": {
    "babel-core": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-istanbul": "0.8.0",
    "babel-istanbul-loader": "0.1.0",
    "babel-loader": "6.2.4",
